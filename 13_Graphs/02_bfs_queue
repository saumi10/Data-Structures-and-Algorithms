#include<bits/stdc++.h>
using namespace std;

// Function to return Breadth First Traversal of given graph.
    vector<int> bfs(vector<vector<int>> &adj) {
        // Code here
        int n=adj.size();
        int vis[n]={0};
        vis[0]=1;
        queue<int>q;
        q.push(0);
        vector<int>bfsresult;
        while(!q.empty()){
            int node=q.front();
            bfsresult.push_back(node);
            q.pop();
            for(auto it:adj[node]){
                if(vis[it]==0){
                    vis[it]=1;
                    q.push(it);
                }
            }
        }
        return bfsresult;
        
    }