 #include<bits/stdc++.h>
using namespace std;
 
 void DFSresult(vector<vector<int>>& adj, int u, vector<int>& result, vector<bool>& visited){
        if(visited[u]==true){
            return;
        }
        visited[u]=true;
        result.push_back(u);
        
        for(int &v:adj[u]){
            if(!visited[v]){
                DFSresult(adj,v,result,visited);
            }
        }
    }
    vector<int> dfs(vector<vector<int>>& adj) {
        // Code here
        vector<int> result;
        vector<bool> visited(adj.size(),false);
        
        DFSresult(adj,0,result,visited);
        return result;
        
    }